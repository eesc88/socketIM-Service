<% include layout.ejs %>
<div class="container">
    <div class="well well-lg">
        1. This system use cookie records the user name, don't worry about other users to see the current page<br/>
        2. Submit the form using the enter key<br/>
        3. I wish you a happy to use
    </div>


    <table class="table">
        <thead>
        <tr>
            <th>Username</th>
            <th>password</th>
            <th>Email</th>
            <th>UserType</th>
            <th>operation</th>
        </tr>
        </thead>
        <tbody>
        <% users.forEach( function (user){ %>
        <tr>
            <td><%= user.get('username') %></td>
            <td><%= user.get('password') %></td>
            <td><%= user.get('email') %></td>
            <td><%= user.get('user_type') %></td>
            <td><a class="btn btn-default " href="/user/edit/<%= user.id %>" type="button">Edit</a>
                <a class="btn btn-default " href="/user/destroy?id=<%= user.id %>" type="button">Del</a>
            </td>
        </tr>
        <% }); %>
        </tbody>
    </table>
</div>

